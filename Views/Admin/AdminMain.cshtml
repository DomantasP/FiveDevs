@{
    ViewData["Title"] = "Administratoriaus pultas";
}


<h2>Administratorius</h2>
<div class="container-fluid display-table">
    <div class="row display-table-row">
        <div class="col-md-2 col-sm-1 hidden-xs display-table-cell v-align box" id="navigation">
            <div class="logo">
                <a hef="home.html">
                    <img src="~/images/admin/admin_logo.png" alt="merkery_logo" class="hidden-xs hidden-sm">
                    <img src="~/images/admin/circle_purple.svg" alt="merkery_logo" class="visible-xs visible-sm circle-logo">
                </a>
            </div>
            <div class="navi">
                <ul>
                    <li id="productsTab"><a href="#/"><i class="fa fa-home" aria-hidden="true"></i><span class="hidden-xs hidden-sm">Prekės</span></a></li>
                    <li id="usersTab"><a href="#/Naudotojai"><i class="fa fa-users" aria-hidden="true"></i><span class="hidden-xs hidden-sm">Naudotojai</span></a></li>
                    <li id="salesTab"><a href="#Nepatvirtinti"><i class="fa fa-bar-chart" aria-hidden="true"></i><span class="hidden-xs hidden-sm">Pardavimai</span></a></li>
                </ul>
            </div>
        </div>
        <div class="col-md-10 col-sm-11 display-table-cell v-align">

            <div class="user-dashboard" id="main">


            </div>
        </div>
    </div>
</div>


<div hidden class="html-spinner">
    <img class="spinnerGif" src="~/lib/LoadingSpinner/spinner2.gif" width="80" height="80" />
    <div class="spinnerText">Prašome palaukti...</div>
</div>

<style>
    .html-spinner {
        position: fixed;
        left: 50%;
        top: 40%;
        background-color: white;
        border-radius: 25px;
        height: 150px;
        width: 150px;
    }

    img.spinnerGif {
        position: absolute;
        left: 35px;
        top: 20px;
    }

    .spinnerText {
        position: absolute;
        left: 30px;
        top: 110px;
    }
</style>

